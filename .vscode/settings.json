{
  "[jinja-html]": {
    "editor.rulers": [120]
  },
  "[json]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[sql]": {
    "editor.defaultFormatter": "ReneSaarsoo.sql-formatter-vsc"
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "autoDocstring.docstringFormat": "sphinx-notypes",
  "cmake.sourceDirectory": "${workspaceFolder}/speedup",
  "coverage-gutters.ignoredPathGlobs": "**/{node_modules,.venv,data}/**",
  "css.styleSheets": [
    "node_modules/bootstrap-icons/font/bootstrap-icons.css",
    "app/static/vite/assets/bootstrap.*.css",
    "app/views/**/*.scss"
  ],
  "editor.formatOnSaveMode": "file",
  "editor.rulers": [88],
  "editor.unicodeHighlight.allowedCharacters": {
    "‚Ä∫": true,
    "¬ª": true,
    "√ó": true,
    "‚ûï": true,
    "ùïè": true
  },
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    ".clang-format": ".clang-format-*",
    ".gitignore": ".gitattributes",
    "*.html.jinja": "${capture}.*",
    "package.json": "pnpm-*.yaml",
    "pyproject.toml": "uv.lock",
    "README.md": "LICENSE"
  },
  "files.exclude": {
    ".*cache/": true,
    ".direnv/": true,
    ".venv/": true,
    "{app,script}/**/*.c": true,
    "**/__pycache__/": true,
    "**/_generated/": true,
    "**/*.egg-info/": true,
    "**/*.html": true,
    "**/*.so": true,
    "**/build/": true,
    "/nix/store/": true,
    "app/static/vite/**/*.js": true,
    "config/locale/download/": true,
    "dist/": true
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "python.analysis.diagnosticMode": "workspace",
  "python.analysis.packageIndexDepths": [
    {
      "depth": 3,
      "includeAllSymbols": false,
      "name": ""
    }
  ],
  "python.experiments.enabled": false,
  "python.terminal.activateEnvironment": false,
  "python.testing.pytestEnabled": true,
  "python.testing.unittestEnabled": false,
  "scss.lint.vendorPrefix": "ignore",
  "SQL-Formatter-VSCode.dataTypeCase": "lower",
  "SQL-Formatter-VSCode.dialect": "postgresql",
  "SQL-Formatter-VSCode.expressionWidth": 80,
  "SQL-Formatter-VSCode.functionCase": "lower",
  "SQL-Formatter-VSCode.keywordCase": "upper",
  "typescript.disableAutomaticTypeAcquisition": true,
  "typescript.tsdk": "node_modules/typescript/lib"
}
