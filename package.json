{
  "name": "openstreetmap-ng",
  "type": "module",
  "private": true,
  "dependencies": {
    "@bufbuild/protobuf": "latest",
    "@noble/hashes": "latest",
    "@preact/signals-core": "latest",
    "@rapideditor/rapid": "latest",
    "@sentry/browser": "latest",
    "bootstrap": "latest",
    "bootstrap-icons": "latest",
    "dicebear": "latest",
    "i18next": "latest",
    "iD": "github:openstreetmap/iD#release",
    "maplibre-gl": "latest",
    "osm-community-index": "latest"
  },
  "devDependencies": {
    "@babel/preset-env": "latest",
    "@babel/preset-typescript": "latest",
    "@bufbuild/protoc-gen-es": "latest",
    "@types/bootstrap": "latest",
    "@types/geojson": "latest",
    "@types/node": "latest",
    "@vitejs/plugin-legacy": "latest",
    "autoprefixer": "latest",
    "browserslist": "latest",
    "caniuse-lite": "latest",
    "core-js": "3.45.0",
    "i18next-conv": "latest",
    "prettier": "latest",
    "sass-embedded": "latest",
    "terser": "latest",
    "typed-query-selector": "latest",
    "typescript": "latest",
    "vite": "npm:rolldown-vite@^7.1.2"
  },
  "scripts": {
    "postinstall": "_pnpm-postinstall"
  },
  "pnpm": {
    "overrides": {
      "vite": "$vite"
    },
    "ignoredBuiltDependencies": [
      "@parcel/watcher",
      "core-js",
      "core-js-bundle",
      "esbuild",
      "sharp"
    ],
    "auditConfig": {
      "ignoreGhsas": [
        "GHSA-jq9w-gjvg-mxr8"
      ]
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "corejs": {
            "version": "3.45.0"
          },
          "modules": false,
          "useBuiltIns": "usage"
        }
      ],
      [
        "@babel/preset-typescript",
        {
          "optimizeConstEnums": true,
          "rewriteImportExtensions": true
        }
      ]
    ]
  },
  "browserslist": [
    "supports abortcontroller and supports bigint and partially supports contentsecuritypolicy2 and partially supports cors and supports es6 and supports fetch and partially supports same-site-cookie-attribute and partially supports webp and since 2019 and not dead"
  ]
}
