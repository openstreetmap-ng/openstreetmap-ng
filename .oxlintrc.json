{
  "ignorePatterns": [
    "app/static/vite/**",
    "app/views/lib/_generated/**",
    "app/views/lib/proto/**",
    "app/views/lib/timezone-bbox.ts",
    "app/views/lib/vector-styles/**"
  ],
  "plugins": [
    "eslint",
    "typescript",
    "unicorn",
    "react",
    "react-perf",
    "oxc",
    "import",
    "jsx-a11y",
    "node",
    "promise"
  ],
  "categories": {
    "correctness": "warn",
    "nursery": "warn",
    "perf": "warn",
    "suspicious": "warn",
    "style": "warn"
  },
  "jsPlugins": ["eslint-plugin-compat"],
  "settings": {
    "lintAllEsApis": true
  },
  "rules": {
    "accessor-pairs": "warn",
    "alt-text": "off",
    "always-return": "off",
    "avoid-new": "off",
    "capitalized-comments": "off",
    "consistent-function-scoping": "off",
    "consistent-type-definitions": "off",
    "consistent-type-imports": "off",
    "consistent-type-specifier-style": "off",
    "curly": "off",
    "exhaustive-deps": "off",
    "exports-last": "off",
    "first": "off",
    "group-exports": "off",
    "id-length": "off",
    "import/no-amd": "warn",
    "import/no-commonjs": "warn",
    "import/no-cycle": "warn",
    "import/no-dynamic-require": "warn",
    "init-declarations": "off",
    "jsx-key": "off",
    "jsx-max-depth": "off",
    "jsx-no-new-array-as-prop": "off",
    "jsx-no-new-function-as-prop": "off",
    "jsx-no-new-object-as-prop": "off",
    "jsx-props-no-spreading": "off",
    "max-params": "off",
    "max-statements": "off",
    "new-cap": "off",
    "no-array-constructor": "warn",
    "no-array-sort": "off",
    "no-async-endpoint-handlers": "off",
    "no-await-expression-member": "off",
    "no-continue": "off",
    "no-duplicate-imports": "off",
    "no-duplicates": "off",
    "no-eq-null": "warn",
    "no-extend-native": "off",
    "no-magic-numbers": "off",
    "no-named-export": "off",
    "no-nested-ternary": "off",
    "no-new": "off",
    "no-null": "off",
    "no-proto": "warn",
    "no-regex-spaces": "warn",
    "no-ternary": "off",
    "no-throw-literal": "warn",
    "no-unassigned-import": "off",
    "no-unsafe-type-assertion": "off",
    "no-unused-expressions": "off",
    "no-var": "warn",
    "number-literal-case": "off",
    "numeric-separators-style": "off",
    "oxc/bad-bitwise-operator": "warn",
    "prefer-await-to-callbacks": "off",
    "prefer-await-to-then": "off",
    "prefer-default-export": "off",
    "prefer-destructuring": "off",
    "prefer-global-this": "off",
    "prefer-optional-chain": "off",
    "promise/spec-only": "warn",
    "radix": "warn",
    "react-in-jsx-scope": "off",
    "react/button-has-type": "warn",
    "react/jsx-no-target-blank": ["warn", { "allowReferrer": true }],
    "react/no-unescaped-entities": "warn",
    "require-await": "warn",
    "sort-imports": "off",
    "sort-keys": "off",
    "switch-case-braces": "off",
    "typescript/ban-types": "warn",
    "typescript/no-deprecated": "warn",
    "typescript/no-dynamic-delete": "warn",
    "typescript/no-import-type-side-effects": "warn",
    "typescript/no-mixed-enums": "warn",
    "typescript/no-namespace": "warn",
    "typescript/no-non-null-asserted-nullish-coalescing": "warn",
    "typescript/no-unsafe-function-type": "warn",
    "typescript/no-var-requires": "warn",
    "typescript/non-nullable-type-assertion-style": "warn",
    "typescript/only-throw-error": "warn",
    "typescript/prefer-includes": "warn",
    "typescript/prefer-literal-enum-member": "warn",
    "typescript/prefer-promise-reject-errors": "warn",
    "typescript/prefer-ts-expect-error": "warn",
    "typescript/related-getter-setter-pairs": "warn",
    "typescript/require-await": "warn",
    "typescript/restrict-plus-operands": "warn",
    "typescript/return-await": "warn",
    "unicorn/new-for-builtins": "warn",
    "unicorn/no-anonymous-default-export": "warn",
    "unicorn/no-immediate-mutation": "warn",
    "unicorn/no-instanceof-array": "warn",
    "unicorn/no-length-as-slice-end": "warn",
    "unicorn/no-nested-ternary": "off",
    "unicorn/no-object-as-default-parameter": "warn",
    "unicorn/no-typeof-undefined": "warn",
    "unicorn/no-unnecessary-array-flat-depth": "warn",
    "unicorn/no-unnecessary-array-splice-count": "warn",
    "unicorn/no-unnecessary-slice-end": "warn",
    "unicorn/no-useless-error-capture-stack-trace": "warn",
    "unicorn/no-useless-promise-resolve-reject": "warn",
    "unicorn/no-useless-undefined": "warn",
    "unicorn/prefer-array-flat": "warn",
    "unicorn/prefer-array-some": "warn",
    "unicorn/prefer-at": "warn",
    "unicorn/prefer-blob-reading-methods": "warn",
    "unicorn/prefer-date-now": "warn",
    "unicorn/prefer-dom-node-append": "warn",
    "unicorn/prefer-dom-node-dataset": "warn",
    "unicorn/prefer-dom-node-remove": "warn",
    "unicorn/prefer-math-min-max": "warn",
    "unicorn/prefer-math-trunc": "warn",
    "unicorn/prefer-modern-math-apis": "warn",
    "unicorn/prefer-number-properties": "warn",
    "unicorn/prefer-prototype-methods": "warn",
    "unicorn/prefer-regexp-test": "warn",
    "unicorn/prefer-string-replace-all": "warn",
    "unicorn/prefer-string-slice": "warn",
    "unicorn/prefer-type-error": "warn"
  },
  "overrides": [
    {
      "files": ["app/views/**/*.ts", "app/views/**/*.tsx"],
      "env": {
        "browser": true,
        "node": false
      },
      "rules": {
        "compat/compat": "error"
      }
    },
    {
      "files": ["vite.config.ts", "app/views/**/*.macro.ts"],
      "env": {
        "browser": false,
        "node": true
      }
    }
  ]
}
