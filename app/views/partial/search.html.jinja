<ul class="search-list social-list list-unstyled mb-0" data-params="{{ params }}">
    {% for result in results %}
    {% set element = result.element %}
    {% set _type, _id = split_typed_element_id(element.typed_id) %}
    {% set point = result.point %}
    <li class="social-entry clickable" {% if point is not none %} data-lon="{{ point.x }}" data-lat="{{ point.y }}"
        {% endif %}>
        <p class="header text-muted d-flex justify-content-between">
            <span>
                <img class="icon d-none" data-feature-icon data-tags="{{ element.tags | tojson }}"
                    data-type="{{ _type }}">
                {{ result.prefix }}
            </span>
            <a class="stretched-link" href="/{{ _type }}/{{ _id }}">
                {{ _type }}/{{ _id }}
            </a>
        </p>
        <p class="body">{{ result.display_name }}</p>
    </li>
    {% endfor %}
</ul>
{% if not results %}
<p>{{ t('results.we_did_not_find_any_results') }}</p>
{% endif %}
